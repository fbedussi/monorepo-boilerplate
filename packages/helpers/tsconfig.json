{
  "compilerOptions": {
    "target": "ESNext",
    "paths": {
      "model": ["../model"]
    },
    "module": "ESNext",
    "moduleResolution": "Node",
    "typeRoots": ["./node_modules/@types"],
    "types": ["node"],
    "declaration": true,
    "outDir": "build",
    "declarationDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
